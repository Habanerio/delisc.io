{
   "semi": true,
   "singleQuote": true,
   "tabWidth": 3,
   "useTabs": false,
   "insertPragma": false,
   "jsxBracketSameLine": true,
   "jsxSingleQuote": true,
   "tsxBracketSameLine": true,
   "tsxSingleQuote": true,
   "printWidth": 80,
   "overrides": [
      {
         "files": [
            "*.js",
            "*.cjs",
            "*.mjs",
            "*.ts",
            "*.tsx",
            "*.cts",
            "*.mts"
         ],
         "options": {
            "tabWidth": 3
         }
      }
   ]
}
